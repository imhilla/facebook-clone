<section class="users-section">
    <ul class="users-list">
        <% @users.each do |user| %>
            <% next if user.id == current_user.id %>
            <li>
                Name: <%= user.name %>
                <span class="profile-link">
                    <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
                </span>
                <%= render "friend_choices", user: user %>
            </li>
        <% end %>
    </ul>
</section>
